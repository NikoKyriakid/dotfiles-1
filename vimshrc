alias rm='rm -i'
alias mv='mv -i'
alias rr='rm -ri'
alias rrf='rm -rf'

alias u='cd ../'
alias uu='cd ../../'
alias uuu='cd ../../../'
alias uuuu='cd ../../../../'

alias ls='ls --color -F'
alias lsa='ls --color -F -a'
alias lsl='ls --color -F -l'
alias lsal='ls --color -F -a -l'

alias be='bundle exec'

alias q='exit'


